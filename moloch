function execute() {
  __command="$@"
  echo $__command
  eval $__command
}
function mstart() {
  command='time sudo ~/moloch-control/moloch-startup.sh '"$@"
  execute $command
}
function mkill() {
  command='time sudo ~/moloch-control/moloch-kill.sh '"$@"
  execute $command
}
function msetup() {
  command='cd ~/moloch-control'
  execute $command
  command='git checkout ubuntu-16-server'
  execute $command
  command='time sudo ~/moloch-control/moloch-setup.sh'
  execute $command
}
function mp() {
  command='ps -Fu daemon'
  execute $command
}
function mc() {
  until curl -sS http://127.0.0.1:9200/_cluster/health?pretty; do
    sleep 0.1
  done
}
